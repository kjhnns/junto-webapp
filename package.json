{
  "name": "junto-webapp",
  "private": true,
  "description": "A simple starter to get up and developing quickly with Gatsby and styled-system",
  "version": "1.0.0",
  "author": "Johannes Klumpe <johannesklumpe@gmail.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/kjhnns/junto-webapp"
  },
  "scripts": {
    "start": "gatsby develop -o",
    "build": "gatsby build",
    "clean": "gatsby clean",
    "test": "jest",
    "test:watch": "jest --watch",
    "format": "prettier --write \"src/**/*.{jsx,js}\"",
    "lint:scripts": "eslint './src/**/*.{jsx,js}'",
    "lint:styles": "stylelint './src/**/*.{jsx,js}'"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged && yarn lint:styles",
      "pre-push": "yarn lint:scripts"
    }
  },
  "dependencies": {
    "@emotion/react": "^11.11.3",
    "@feedback-fish/react": "^1.2.2",
    "@reach/dialog": "^0.18.0",
    "@rebass/forms": "^4.0.6",
    "@styled-system/prop-types": "^5.1.5",
    "@styled-system/theme-get": "^5.1.2",
    "axios": "^1.9.0",
    "dotenv": "^16.5.0",
    "firebase": "^11.8.1",
    "formik": "^2.4.6",
    "framer-motion": "^12.16.0",
    "gatsby": "^5.14.3",
    "gatsby-image": "^3.11.0",
    "gatsby-plugin-catch-links": "^5.14.0",
    "gatsby-plugin-create-client-paths": "^4.9.0",
    "gatsby-plugin-firebase": "^0.2.0-beta.4",
    "gatsby-plugin-google-tagmanager": "^5.14.0",
    "gatsby-plugin-lodash": "^6.14.0",
    "gatsby-plugin-manifest": "^5.14.0",
    "gatsby-plugin-offline": "^6.14.0",
    "gatsby-plugin-react-helmet": "^6.14.0",
    "gatsby-plugin-sharp": "^5.14.0",
    "gatsby-plugin-sitemap": "^6.14.0",
    "gatsby-plugin-theme-ui": "^0.17.2",
    "gatsby-source-filesystem": "^5.14.0",
    "gatsby-transformer-sharp": "^5.14.0",
    "moment": "^2.30.1",
    "prop-types": "^15.8.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-firebaseui": "^6.0.0",
    "react-ga": "^3.3.1",
    "react-helmet": "^6.1.0",
    "react-typography": "^0.16.23",
    "rebass": "^4.0.7",
    "theme-ui": "^0.17.2",
    "typeface-inter": "^3.18.1",
    "typography": "^0.16.24",
    "yup": "^1.6.1"
  },
  "devDependencies": {
    "@babel/core": "^7.27.4",
    "@testing-library/dom": "^9.3.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^29.7.0",
    "babel-preset-gatsby": "^3.14.0",
    "eslint": "^9.28.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^10.1.5",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-compat": "^6.0.2",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jest": "^28.12.0",
    "eslint-plugin-jsx": "^0.1.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-prettier": "^5.4.1",
    "eslint-plugin-promise": "^7.2.1",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-styled-components": "^7.2.0",
    "prettier": "^3.5.3",
    "pretty-quick": "^4.2.2",
    "stylelint": "^16.20.0",
    "stylelint-config-recommended": "^16.0.0",
    "stylelint-config-standard": "^38.0.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-processor-styled-components": "^1.10.0"
  }
}
